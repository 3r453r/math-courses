# Anthropic API Key (optional - can also be set in the browser UI)
ANTHROPIC_API_KEY=

# Database URL (default: SQLite file in prisma directory)
DATABASE_URL="file:./dev.db"

# Auth.js â€” required for authentication
AUTH_SECRET=                              # Generate with: openssl rand -base64 32

# OAuth providers (configure at least one for production)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
DISCORD_CLIENT_ID=
DISCORD_CLIENT_SECRET=

# Dev bypass (set to "true" to skip auth in development/testing)
AUTH_DEV_BYPASS=false

# Stripe configuration
STRIPE_SECRET_KEY=
STRIPE_PRICE_ID=
STRIPE_WEBHOOK_SECRET=
